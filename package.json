{
  "name": "atrangi-eventz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=web...",
    "dev:admin": "turbo dev --filter=admin...",
    "lint": "turbo lint",
    "check-types": "turbo check-types",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui:add": "node scripts/ui-add.js",
    "test": "node packages/server/infra/src/test.js"
  },
  "devDependencies": {
    "@atrangi/eslint-config": "workspace:*",
    "@atrangi/typescript-config": "workspace:*",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "turbo": "^2.8.10",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@10.30.0",
  "engines": {
    "node": ">=24"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^19.2.7",
      "@types/react-dom": "^19.2.3"
    }
  }
}
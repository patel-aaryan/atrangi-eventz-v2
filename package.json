{
  "name": "my-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=web...",
    "dev:admin": "turbo dev --filter=admin...",
    "lint": "turbo lint",
    "check-types": "turbo check-types",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui:add": "node scripts/ui-add.js"
  },
  "devDependencies": {
    "@atrangi/eslint-config": "workspace:*",
    "@atrangi/typescript-config": "workspace:*",
    "prettier": "^3.7.4",
    "turbo": "^2.6.3",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=24"
  }
}